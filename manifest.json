{
	"manifest_version": 3,
	"name": "Hello World Extension",
	"version": "1.0",
	"description": "A hello world extension",
	"icons": {
		"16": "16x16.png",
		"48": "48x48.png",
		"128": "128x128.png"
	},
	"permissions": ["storage"],
	"background": {
	  "service_worker": "background.js"
	},
	"action": {
	  "default_popup": "popup.html",
	  "default_icon": "16x16.png"
  	}
}